{
    "name": "express-typescript-template",
    "version": "1.0.0",
    "description": "Express.js TypeScript template with health endpoint",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
        "db:create": "ts-node -r tsconfig-paths/register src/scripts/create-tables.ts",
        "db:create-google-meet": "ts-node -r tsconfig-paths/register src/scripts/create-google-meet-tables.ts",
        "db:update-google-meet": "ts-node -r tsconfig-paths/register src/scripts/update-google-meet-table.ts",
        "db:add-recording-columns": "ts-node -r tsconfig-paths/register src/scripts/add-recording-columns.ts",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "test": "jest",
        "seed": "ts-node -r tsconfig-paths/register seed.ts"
    },
    "keywords": [
        "express",
        "typescript",
        "nodejs",
        "api"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "bcrypt": "^6.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "google-auth-library": "^10.3.0",
        "google-calendar-api": "^0.0.1",
        "googleapis": "^160.0.0",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "nodemailer": "^7.0.6",
        "pg": "^8.11.0",
        "razorpay": "^2.9.6",
        "reflect-metadata": "^0.1.13",
        "socket.io": "^4.7.5",
        "typeorm": "^0.3.17"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.4.0",
        "@types/nodemailer": "^7.0.1",
        "@types/pg": "^8.10.2",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.44.0",
        "jest": "^29.6.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.6"
    }
}
